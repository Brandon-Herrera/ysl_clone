<div class="left-panel">
    <h1><a href="">SAINT LAURENT</a></h1>
    <ul data-level="1">
        <% for (let i = 0; i < categories.filterCategories('leftSidebar', 1).length; i++) { %>
            <li>
                <button data-level="1" data-active="false"><%= categories.filterCategories('leftSidebar', 1)[i]['name'].toUpperCase() %></button>
                <div class="subnav" data-expanded="false">
                    <ul data-level="2">
                        <% for (let j = 0; j < categories.filterCategories('leftSidebar', 2).length; j++) { %> 
                        <li>
                            <button data-level="2"><%= categories.filterCategories('leftSidebar', 2)[i]['name'].toUpperCase() %> </button>
                        </li>
                        <% } %> 
                    </ul>
                </div>
            </li>
        <% } %> 
    </ul>
</div>