<div class="left-panel-wrapper">
    <div class="left-panel"> 
        <h1><a href="/">SAINT LAURENT</a></h1>
        <ul data-level="1">
            <% for (let i = 0; i < categories.catsByAncestors('leftSidebar').length; i++) { %>
                <% const levelOneCat = categories.catsByAncestors('leftSidebar')[i]['name'] %>
                <li>
                    <button data-level="1" data-active="false"><%= levelOneCat.toUpperCase() %></button>
                    <div class="subnav" data-expanded="false">
                        <ul data-level="2">
                            <% for (let j = 0; j < categories.catsByAncestors('leftSidebar', levelOneCat).length; j++) { %>
                                <% const levelTwoCat = categories.catsByAncestors('leftSidebar', levelOneCat)[j]['name'] %>
                                <li>
                                    <button data-level="2" data-active="false"><%= levelTwoCat.toUpperCase() %></button>
                                    <div class="subnav" data-expanded="false">
                                        <ul data-level="3">
                                            <% for (let k = 0; k < categories.catsByAncestors('leftSidebar', levelOneCat, levelTwoCat).length; k++) { %>
                                                <% const levelThreeCat = categories.catsByAncestors('leftSidebar', levelOneCat, levelTwoCat)[k]['name'] %>
                                                <li>
                                                    <button data-level="3" data-active="false"><%= levelThreeCat.toUpperCase() %></button>
                                                    <div class="subnav" data-expanded="false">
                                                        <ul data-level="4">
                                                            <% for (let l = 0; l < categories.catsByAncestors('leftSidebar', levelOneCat, levelTwoCat, levelThreeCat).length; l++) { %>
                                                                <% const levelFourCat = categories.catsByAncestors('leftSidebar', levelOneCat, levelTwoCat, levelThreeCat)[l]['name'] %>
                                                                <li>
                                                                    <button data-level="4" data-active="false"><%= levelFourCat.toUpperCase() %></button>
                                                                    <div class="subnav" data-expanded="false">
                                                                        <ul data-level="5">
                                                                            <% for (let m = 0; m < categories.catsByAncestors('leftSidebar', levelOneCat, levelTwoCat, levelThreeCat, levelFourCat).length; m++) { %>
                                                                                <% const levelFiveCat = categories.catsByAncestors('leftSidebar', levelOneCat, levelTwoCat, levelThreeCat, levelFourCat)[m]['name'] %>
                                                                                <li>
                                                                                    <button data-level="5" data-active="false"><%= levelFiveCat.toUpperCase() %></button>
                                                                                    <div class="subnav" data-expanded="false"></div>
                                                                                        <ul data-level="6">
                                                                                            <% for (let n = 0; n < categories.catsByAncestors('leftSidebar', levelOneCat, levelTwoCat, levelThreeCat, levelFourCat, levelFiveCat).length; n++) { %>
                                                                                                <% const levelSixCat = categories.catsByAncestors('leftSidebar', levelOneCat, levelTwoCat, levelThreeCat, levelFourCat, levelFiveCat)[n]['name'] %>
                                                                                                <li>
                                                                                                    <button data-level="6" data-active="false"><%= levelSixCat.toUpperCase() %></button>
                                                                                                    <div class="subnav" data-expanded="false"></div>
                                                                                                </li>
                                                                                            <% } %> 
                                                                                        </ul>
                                                                                </li>
                                                                            <% } %> 
                                                                        </ul>
                                                                    </div>
                                                                </li>
                                                            <% } %> 
                                                        </ul>
                                                    </div>
                                                </li>
                                            <% } %> 
                                        </ul>
                                </li>
                            <% } %> 
                        </ul>
                    </div>
                </li>
            <% } %> 
        </ul>
    </div>
</div>